                code.addRCode("sum8<-summary(DFcointegration)@teststat[1]");

                code.addRCode("sum<-c(sum1,sum2,sum3,sum4,sum5,sum6,sum7,sum8)");
                caller.setRCode(code);
                caller.runAndReturnResult("sum");
